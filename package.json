{
  "name": "vue-demo-collection",
  "description": "A collection of Vue.js demos",
  "author": "xiaoluoboding <xiaoluoboding@gmail.com>",
  "scripts": {
    "github-file-explorer": "webpack-dev-server --config github-file-explorer/webpack.config.js --content-base github-file-explorer --inline --hot --port 8090",
    "shopping-cart": "webpack-dev-server --config shopping-cart/webpack.config.js --content-base shopping-cart --inline --hot --port 8090",
    "vuex-mongoose": "webpack-dev-server --config vuex-mongoose/webpack.config.js --content-base vuex-mongoose --inline --hot --port 8090",
    "financial-tools": "webpack-dev-server --config financial-tools/webpack.config.js --content-base financial-tools --inline --hot --port 8090",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "vue": "^2.2.1",
    "vue-resource": "^1.3.1",
    "vue-router": "^2.5.2",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.2.0",
    "vue-loader": "^11.1.4",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
